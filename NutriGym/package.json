{
  "name": "app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"next dev\" \"node ./src/app/api/conexion.js\"",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "migrate": "npx sequelize-cli db:migrate --migrations-path=./src/db/migrations",
    "down": "npx sequelize-cli db:migrate:undo:all --migrations-path=./src/db/migrations",
    "seed": "npx sequelize-cli db:seed:all --seeders-path ./src/db/seeders",
    "undo-seed": "npx sequelize-cli db:seed:undo --seeders-path ./src/db/seeders",
    "undo-all-seeds": "npx sequelize-cli db:seed:undo:all --seeders-path ./src/db/seeders"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.6.0",
    "@fortawesome/free-solid-svg-icons": "^6.6.0",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@material-tailwind/react": "^2.1.10",
    "autoprefixer": "^10.4.20",
    "bcrypt": "^5.1.1",
    "concurrency": "^0.1.4",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.21.1",
    "express-validator": "^7.2.0",
    "framer-motion": "^11.11.4",
    "jsonwebtoken": "^9.0.2",
    "jspdf": "^2.5.2",
    "jspdf-autotable": "^3.8.4",
    "lucide-react": "^0.447.0",
    "next": "^14.2.15",
    "next-auth": "^4.24.8",
    "pdfjs-dist": "^4.7.76",
    "pg": "^8.13.1",
    "pg-hstore": "^2.3.4",
    "react": "^18.3.1",
    "react-chatbot-kit": "^2.2.2",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.27.0",
    "sequelize": "^6.37.5",
    "sweetalert2": "^11.14.4",
    "tailwind-merge": "^2.5.3"
  },
  "devDependencies": {
    "@types/node": "22.7.4",
    "@types/react": "18.3.11",
    "concurrently": "^9.0.1",
    "daisyui": "^4.12.14",
    "eslint": "^8",
    "eslint-config-next": "14.2.14",
    "postcss": "^8.4.47",
    "sequelize-cli": "^6.6.2",
    "tailwindcss": "^3.4.13"
  }
